}
.card-bg .bgMask {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 540rpx;
  border-radius:8rpx;
  z-index: 10;
  background:linear-gradient(227deg,rgba(255,255,255,1) 0%,rgba(127,127,127,1) 100%);
  opacity: 0.25;
  mix-blend-mode: overlay;
}

