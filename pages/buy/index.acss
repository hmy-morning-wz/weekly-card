}
.card-box .colorMask {
  position: absolute;
  top: 0;
  left: 0;
  width: 592rpx;
  height: 332rpx;
  border-radius:8rpx;
  z-index: 10;
  opacity: 0.4;
  mix-blend-mode: multiply;
}
.card-box image{
  width: 592rpx;
